{
  "name": "backend-template",
  "version": "1.0.0",
  "description": "a template to create api with authentication",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "lint": "standard --fix",
    "prepare": "husky install"
  },
  "author": "Mateo2131",
  "license": "MIT",
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "standard": "^17.0.0",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.0",
    "mongoose-unique-validator": "^3.1.0"
  },
  "imports": {
    "#Controllers/*": [
      "./src/controllers/*"
    ],
    "#Models/*": [
      "./src/models/*"
    ],
    "#Utils/*": [
      "./src/utils/*"
    ],
    "#Middlewares/*": [
      "./src/middlewares/*"
    ]
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix"
    ]
  }
}
